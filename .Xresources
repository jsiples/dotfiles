!URxvt*font: -*-terminus-medium-*-*-*-24-*-*-*-*-*-*-*
!URxvt*boldFont: -*-terminus-bold-*-*-*-24-*-*-*-*-*-*-*
URxvt*font: xft:Hack:pixelsize=18:antialias=true
URxvt*cursorBlink: true
URxvt*letterSpace: 0
URxvt*cursorUnderline: true
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*urgentOnBell: true
URxvt*depth: 32
URxvt.background: #000000
URxvt.foreground: #ffffff
URxvt*saveline: 10000
URxvt*geometry: 120x32

! URL functions
URxvt.perl-ext-common: default,matcher,clipboard,url-select,keyboard-select,font-size
URxvt.matcher.button: 1
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: true
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search

! special
*.foreground:   #93a1a1
*.background:   #002b36
*.cursorColor:  #93a1a1

! black
*.color0:       #002b36
*.color8:       #657b83

! red
*.color1:       #dc322f
*.color9:       #dc322f

! green
*.color2:       #859900
*.color10:      #859900

! yellow
*.color3:       #b58900
*.color11:      #b58900

! blue
*.color4:       #268bd2
*.color12:      #268bd2

! magenta
*.color5:       #6c71c4
*.color13:      #6c71c4

! cyan
*.color6:       #2aa198
*.color14:      #2aa198

! white
*.color7:       #93a1a1
*.color15:      #fdf6e3

